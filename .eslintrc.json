{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-console": "warn",
    "eslint linebreak-style": [
      "error", "windows"
    ],
    "linebreak-style": 0
  }
}